<h1><%= @doctor.name %></h1>
<h2>Patients:</h2>
<ul>
    <% @doctor.patients.each do |patient| %>
        <li><%= link_to patient.name, patient_path(patient) %></li>
    <% end %>
</ul>
<h2>Appointments:</h2>
<ul>
    <% @doctor.appointments.each do |appointment| %>
        <li><%= link_to appointment.display_datetime, appointment_path(appointment) %></li>
    <% end %>
</ul>